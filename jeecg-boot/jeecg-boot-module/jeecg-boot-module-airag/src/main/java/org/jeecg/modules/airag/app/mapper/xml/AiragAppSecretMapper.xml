<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.airag.app.mapper.AiragAppSecretMapper">

    <!-- 根据应用ID查询启用的密钥配置 -->
    <select id="getEnabledByAppId" resultType="org.jeecg.modules.airag.app.entity.AiragAppSecret">
        SELECT * FROM airag_app_secret
        WHERE app_id = #{appId}
          AND enabled = 1
        LIMIT 1
    </select>

</mapper>
